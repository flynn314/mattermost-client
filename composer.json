{
	"name": "flynn314/mattermost-client",
	"description": "Mattermost client",
	"type": "library",
	"license": "MIT",
	"require": {
		"php": "^7.4",
		"ext-json": "*",
		"guzzlehttp/guzzle": "^7.5"
	},
	"require-dev": {
		"phpunit/phpunit": "~6.0",
		"vlucas/phpdotenv": "^2.5",
		"larapack/dd": "^1.1"
	},
	"autoload": {
		"psr-4": {
			"Flynn314\\Mattermost\\": "src"
		}
	},
	"autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
	"minimum-stability": "dev",
	"prefer-stable": true
}
